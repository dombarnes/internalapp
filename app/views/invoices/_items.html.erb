<table id="invoice-items">
	<thead>
		<td>Qty</td>
		<td>Item</td>
		<td>Price</td>
		<td>Description</td>
		<td>Tax Rate</td>
		<td>Total</td>
	</th>
<% @items.each do |item| %>
	 <tr>
	 	<td><%= item.quantity %></td>
	    <td><%= item.unit_type %></td>
	    <td><%= item.unit_price %></td>
	    <td><%= item.details %></td>
	    <td><%= item.tax_rate %></td>
		<td><%= @invoice.total %></td>
	</tr>
<% end %>
</table>
